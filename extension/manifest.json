{
    "manifest_version": 2,

    "name": "UR Great Extension",
    "description": "So great",
    "version": "1.0",

    "permissions": [
	"activeTab"
    ],

    "browser_action": {
        "default_icon": {
            "16": "icon.png",
            "24": "icon.png",
            "32": "icon.png"
        },
        "default_title": "So great"
    },
    "content_scripts":  [
	{
	    "matches":["https://www.yahoo.com/*"],
	    "js":["bundle.js"]
	}
    ]
}
